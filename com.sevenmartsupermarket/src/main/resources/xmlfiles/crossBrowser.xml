<suite name="CrossBrowser" thread-count="5" parallel="tests">
	<listeners>
		<listener
			class-name="com.sevenmartsupermarket.listerners.ExtentReporter"></listener>
	</listeners>
	<test name="Running in Chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>


		</classes>
	</test>
	<test name="Running in Edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>


		</classes>
	</test>
	<test name="Running in FireFox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
</classes>
	</test>
</suite>